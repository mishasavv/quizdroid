<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.washington.mikhail3.quizdroid" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MathQuizActivity"
            android:label="@string/title_activity_math_quiz"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.washington.mikhail3.quizdroid.MainActivity" />
        </activity>
        <activity
            android:name=".PhysicsQuizActivity"
            android:label="@string/title_activity_physics_quiz"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.washington.mikhail3.quizdroid.MainActivity" />
        </activity>
        <activity
            android:name=".HeroesQuizActivity"
            android:label="@string/title_activity_heroes_quiz"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.washington.mikhail3.quizdroid.MainActivity" />
        </activity>
        <activity
            android:name=".QuizInfoActivity"
            android:label="@string/title_activity_quiz_info"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.washington.mikhail3.quizdroid.MainActivity" />
        </activity>
        <activity
            android:name=".question"
            android:label="@string/title_activity_question"
            android:parentActivityName=".QuizInfoActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.washington.mikhail3.quizdroid.QuizInfoActivity" />
        </activity>
        <activity
            android:name=".ScorePageActivity"
            android:label="@string/title_activity_score_page"
            android:parentActivityName=".question" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.washington.mikhail3.quizdroid.question" />
        </activity>
    </application>

</manifest>
